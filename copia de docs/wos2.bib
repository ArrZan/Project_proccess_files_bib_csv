@article{ WOS:000554873900001,
Author = {Dwekat, Aladdin and Segui-Mas, Elies and Tormo-Carbo, Guillermina and
   Carmona, Pedro},
Title = {Corporate Governance Configurations and Corporate Social Responsibility
   Disclosure: Qualitative Comparative Analysis of Audit Committee and
   Board characteristics},
