@article{ WOS:000619996300001,
Author = {Wu, Jie and Triana, Maria del Carmen and Richard, Orlando C. and Yu,
   Luman},
Title = {Gender Faultline Strength on Boards of Directors and Strategic Change:
   The Role of Environmental Conditions},
Journal = {GROUP \& ORGANIZATION MANAGEMENT},
