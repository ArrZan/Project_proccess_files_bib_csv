@article{WOS:000639112300117,
author={Thi Minh Thu Nguyen and Thi Tuong Anh Nguyen and Thi Thuy Vinh Nguyen
and Huong Giang Pham},

title={Foreign Direct Investment - Small and Medium Enterprises Linkages and
Global Value Chain Participation: Evidence from Vietnam},

