@article{WOS:000742482400011,
author={Mitrovic, Vladimir and Mitrovic, Jovan},

title={DEFINING THE NOTIONS OF MANAGEMENT OF SUPPLY CHAINS, VALUE CHAINS AND
CLUSTERS IN AGRICULTURE},

journal={CASOPIS ZA EKONOMIJU I TRZISNE KOMUNIKACIJE},

