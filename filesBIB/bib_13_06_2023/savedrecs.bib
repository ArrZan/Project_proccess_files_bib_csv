@article{ WOS:000916185400023,
Author = {White, Erin M. and Esposito, Andrew C. and Kurbatov, Vadim and Wang,
   Xujun and Caty, Michael G. and Laurans, Maxwell and Yoo, Peter S.},
Title = {How I Learned is How I Teach - Perspectives on How Faculty Surgeons
   Approach Informed Consent Education},
Journal = {JOURNAL OF SURGICAL EDUCATION},
Year = {2022},
Volume = {79},
Number = {6},
