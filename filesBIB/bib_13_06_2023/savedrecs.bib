Title = {Collaborative Learning- and Quiz-based Teaching Strategy in Biochemistry
   and Molecular Biology},
Journal = {INDIAN JOURNAL OF PHARMACEUTICAL EDUCATION AND RESEARCH},
Year = {2019},
