@article{ WOS:000583096300003,
Author = {Crisci, Serena and Kruzik, Jakub and Pecha, Marek and Horak, David},
Title = {Comparison of active-set and gradient projection-based algorithms for
   box-constrained quadratic programming},
Journal = {SOFT COMPUTING},
