@article{ WOS:000330104500014,
Author = {Huang, Grace C. and Newman, Lori R. and Schwartzstein, Richard M.},
Title = {Critical Thinking in Health Professions Education: Summary and Consensus
   Statements of the Millennium Conference 2011},
Journal = {TEACHING AND LEARNING IN MEDICINE},
