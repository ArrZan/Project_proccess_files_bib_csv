@article{ WOS:000501314300021,
Author = {Shaw, Andrew C. and McQuade, Jennifer L. and Reilley, Matthew J. and
   Nixon, Burke and Baile, Walter F. and Epner, Daniel E.},
Title = {Integrating Storytelling into a Communication Skills Teaching Program
   for Medical Oncology Fellows},
Journal = {JOURNAL OF CANCER EDUCATION},
