@article{ WOS:000660872200001,
Author = {Schafer, Frank and Sekatski, Pavel and Koppenhofer, Martin and Bruder,
   Christoph and Kloc, Michal},
Title = {Control of stochastic quantum dynamics by differentiable programming},
