@article{ WOS:000475159200001,
Author = {Huang, Pin-Hsiang and Haywood, Matthew and O'Sullivan, Anthony and
   Shulruf, Boaz},
Title = {A meta-analysis for comparing effective teaching in clinical education},
Journal = {MEDICAL TEACHER},
Year = {2019},
Volume = {41},
Number = {10},
Pages = {1129-1142},
Month = {OCT 3},
Abstract = {Aim: Many factors affect learning outcomes, however studies comparing
   the effectiveness of different clinical teaching methods are limited. We
   utilize the list of influences on educational achievement compiled by
   John Hattie to inform a meta-analysis of learning effect sizes (ESs)
   associated with teaching-learning factors (TLFs) in clinical education.
   Methods: A literature search was conducted in PubMed to identify
   articles examining clinically relevant TLFs. Selection criteria were
   applied to identify learner-focused studies, with subsequent
   categorization by study design (pretest-posttest or controlled group).
   The Cohen's ES (d) for each TLF was extracted and a pooled ES
   determined. Results: From 3454 studies, 132 suitable articles enabled
   analysis of 16 TLFs' ESs. In general, ESs derived from pretest-posttest
   data were larger than those from controlled group designs, probably due
   to learner maturation effect. The TLFs of mastery learning, small group
   learning and goal settings possessed the largest ESs (d >= 0.8), while
   worked examples, play programs, questioning, concept mapping,
   meta-cognitive strategies, visual-perception programs and teaching
   strategies demonstrated ESs between 0.4 and 0.8. Conclusions: This is
   the first study to provide a rigorous and comprehensive overview of the
   effectiveness of TLFs in clinical education. We discuss the practical
   traits shared by effective TLFs which may assist teaching design.},
Publisher = {TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND},
Type = {Article},
