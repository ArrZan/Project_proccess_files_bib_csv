@article{ WOS:000991010200003,
Author = {Houpert, Remi and Almont, Thierry and Mesenge, Christian and
   Kleinebreil, Line and Forlini, Laurence and Magnone, Bruno and Leroux,
   Vincent and Vestris, Mylene and Montabord, Christelle and Carmenate,
