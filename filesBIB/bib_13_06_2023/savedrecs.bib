@article{ WOS:000346785000006,
Author = {Moran, Thomas Eugene and Taliaferro, Andrea R. and Pate, Joshua R.},
Title = {Confronting Physical Activity Programming Barriers for People with
   Disabilities: The Empowerment Model},
