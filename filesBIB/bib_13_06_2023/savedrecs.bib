@article{ WOS:000600647800003,
Author = {Goh, Tan Leng and Scrabis-Fletcher, Kristin},
Title = {Community of Practice: Preparing Preservice Teachers to Lead Movement
   Integration in a School-University Partnered Program},
Journal = {JOURNAL OF TEACHING IN PHYSICAL EDUCATION},
