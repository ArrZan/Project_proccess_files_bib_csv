@article{ WOS:000370737300003,
Author = {Poza-Lujan, Jose-Luis and Calafate, Carlos T. and Posadas-Yaguee,
   Juan-Luis and Cano, Juan-Carlos},
Title = {Assessing the Impact of Continuous Evaluation Strategies: Tradeoff
   Between Student Performance and Instructor Effort},
Journal = {IEEE TRANSACTIONS ON EDUCATION},
Year = {2016},
Volume = {59},
Number = {1},
Pages = {17-23},
Month = {FEB},
Abstract = {Current opinion on undergraduate studies has led to a reformulation of
   teaching methodologies to base them not just on learning, but also on
   skills and competencies. In this approach, the teaching/learning process
   should accomplish both knowledge assimilation and skill development.
   Previous works demonstrated that a strategy that uses continuous
   evaluation is able to meet both objectives. However, those studies did
   not evaluate and quantify the additional effort required to implement
   such strategies. This paper evaluates the additional instructor effort
   required when implementing continuous evaluation in a first-year
   Computer Fundamentals course in the Computer Engineering degree program
   at the Technical University of Valencia, Spain. The experiment
   quantifies how instructor workload increases under different continuous
   evaluation strategies and how this affects the overall student grade.
   Both the ``standard{''} continuous evaluation method and the intensive
   continuous evaluation method are analyzed, the latter being a proposal
   that builds upon the standard method by increasing the number of tests
   and examinations. The results obtained reveal that continuous evaluation
   improves student grades but that intensive continuous evaluation is
   liable to generate an excessive instructor overload without having a
   significant impact on student scores.},
