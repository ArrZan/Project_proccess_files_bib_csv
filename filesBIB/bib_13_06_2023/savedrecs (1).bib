Author = {Sharma, Shreela V. and Chow, Joanne and Pomeroy, Michael and Raber,
   Margaret and Salako, David and Markham, Christine},
Title = {Lessons Learned From the Implementation of Brighter Bites: A Food Co-op
   to Increase Access to Fruits and Vegetables and Nutrition Education
   Among Low-Income Children and Their Families},
Journal = {JOURNAL OF SCHOOL HEALTH},
