@article{ WOS:000363079400009,
Author = {Dixon, Kathleen A. and Cotton, Antoinette and Moroney, Robyn and
   Salamonson, Yenna},
Title = {The experience of sessional teachers in nursing: A qualitative study},
Journal = {NURSE EDUCATION TODAY},
Year = {2015},
Volume = {35},
