@article{ WOS:000541303300001,
Author = {Rezaee Jordehi, Ahmad},
Title = {Particle swarm optimisation with opposition learning-based strategy: an
   efficient optimisation algorithm for day-ahead scheduling and
   reconfiguration in active distribution systems},
Journal = {SOFT COMPUTING},
