Author = {Stephenson, Norda S. and Miller, Imron R. and Sadler-McKnight, Novelette
   P.},
Title = {Impact of Peer-Led Team Learning and the Science Writing and Workshop
   Template on the Critical Thinking Skills of First-Year Chemistry
   Students},
Journal = {JOURNAL OF CHEMICAL EDUCATION},
Year = {2019},
Volume = {96},
Number = {5},
Pages = {841-849},
Month = {MAY},
Abstract = {The Peer-Led Team Learning (PLTL) and the Science Writing and Workshop
   Template (SWWT) are two active learning instructional approaches which
   combine writing inquiry, collaboration, and reflection, elements which
   have been associated with critical thinking development. In this study,
   we used a quasi-experimental pretest post-test design to investigate the
   impact of the implementation of these two approaches on the critical
   thinking skills of first-year chemistry students, measured using the
   California Critical Thinking Skills Test (CCTST). The results indicate
   that implementation of the PLTL and the SWWT instructional strategies
   led to significantly higher gains in critical thinking.},
