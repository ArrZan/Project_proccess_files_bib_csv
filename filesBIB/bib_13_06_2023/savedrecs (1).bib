@article{ WOS:000864631900001,
Author = {Ernesto, Julia Vaz and Machado, Daiane Silva and Lellis-Santos, Camilo},
Title = {The presidential election of the human body: applying contextual
   learning to promote connections between the student and the curricular
   content},
Journal = {ADVANCES IN PHYSIOLOGY EDUCATION},
