@article{ WOS:000403457100036,
Author = {Lai, Siyan and Lai, Guangda and Lu, Fangzhou and Shen, Guojun and Jin,
   Jing and Lin, Xiaola},
Title = {A BSP model graph processing system on many cores},
Journal = {CLUSTER COMPUTING-THE JOURNAL OF NETWORKS SOFTWARE TOOLS AND
   APPLICATIONS},
Year = {2017},
