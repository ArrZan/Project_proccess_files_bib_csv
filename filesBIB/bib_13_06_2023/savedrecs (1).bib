@article{ WOS:000891640600002,
Author = {Amaya-Gomez, Rafael and Dumar, Vivian and Sanchez-Silva, Mauricio and
   Torres-Cuello, Maria Alejandra and Avila, Alba and Munoz, Felipe},
Title = {An analysis of engineering students' risk perception to support process
   safety learning process},
