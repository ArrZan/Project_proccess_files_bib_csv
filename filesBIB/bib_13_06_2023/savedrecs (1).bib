@article{ WOS:000766635400004,
Author = {Jia, Qiangang and Chen, Sijie and Yan, Zheng and Li, Yiyan},
Title = {Optimal Incentive Strategy in Cloud-Edge Integrated Demand Response
   Framework for Residential Air Conditioning Loads},
Journal = {IEEE TRANSACTIONS ON CLOUD COMPUTING},
