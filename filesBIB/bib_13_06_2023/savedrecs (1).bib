@article{ WOS:000344591500003,
Author = {Hoyek, Nady and Collet, Christian and Di Rienzo, Franck and De Almeida,
   Mickael and Guillot, Aymeric},
Title = {Effectiveness of Three-Dimensional Digital Animation in Teaching Human
   Anatomy in an Authentic Classroom Context},
