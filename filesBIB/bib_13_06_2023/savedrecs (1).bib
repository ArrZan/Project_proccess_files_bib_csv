@article{ WOS:000872394000004,
Author = {Ma, Kaiwen and Sahinidis, V, Nikolaos and Bindlish, Rahul and Bury,
   Scott J. and Haghpanah, Reza and Rajagopalan, Sreekanth},
Title = {Data-driven strategies for extractive distillation unit optimization},
