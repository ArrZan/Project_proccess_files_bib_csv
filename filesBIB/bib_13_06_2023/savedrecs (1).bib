@article{ WOS:000355057300012,
Author = {Fergus, Suzanne and Kellett, Kathryn and Gerhard, Ute},
Title = {The Khat and Meow Meow Tale: Teaching the Relevance of Chemistry through
   Novel Recreational Drugs},
Journal = {JOURNAL OF CHEMICAL EDUCATION},
Year = {2015},
Volume = {92},
Number = {5},
Pages = {843-848},
Month = {MAY},
Abstract = {Using current research and real-life scenarios to motivate students to
   understand chemistry principles is a key strategy in learning and
   teaching. An illustration of psychoactive drugs referred to as ``legal
   highs{''} used in the U.K. and Europe is presented to highlight key
   chemistry principles and relate the importance of chemistry research in
   healthcare. Specific topics include stereochemistry, H-1 NMR
   characterization, organic `synthesis, and functional groups, which can
   be used in chemistry curricula for high school students and also in
   higher education programs. A contextualized example focused on
   mephedrone is used during an interpretative spectroscopy exercise in a
   second-year bioscience program. Student evaluations on the use of this
