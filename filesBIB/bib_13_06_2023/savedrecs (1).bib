Author = {Zhou, Xinsen and Gui, Wenyong and Heidari, Ali Asghar and Cai, Zhennao
   and Elmannai, Hela and Hamdi, Monia and Liang, Guoxi and Chen, Huiling},
Title = {Advanced orthogonal learning and Gaussian barebone hunger games for
   engineering design},
