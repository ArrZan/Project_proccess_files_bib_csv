@article{ WOS:000360824700003,
Author = {Azzi, Alain J. and Ramnanan, Christopher J. and Smith, Jennifer and
   Dionne, Eric and Jalali, Alireza},
Title = {To quiz or not to quiz: Formative tests help detect students at risk of
   failing the clinical anatomy course},
Journal = {ANATOMICAL SCIENCES EDUCATION},
Year = {2015},
Volume = {8},
Number = {5},
Pages = {413-420},
Month = {SEP-OCT},
Abstract = {Through a modified team-based learning (TBL) in the anatomy
   pre-clerkship curriculum, formative evaluations are utilized in the
   University of Ottawa Faculty of Medicine to assess and predict students'
   outcomes on summative examinations. The purpose of this study was to
   determine the efficiency of formative assessments to predict student's
   performance on summative examinations, during the first two semesters of
   medical school. Formative assessments included multiple-choice quizzes
   (MCQ) for each laboratory session and a practical midterm examination
   (MIDTERM), while the summative assessment corresponded to the final
   practical examination (FINAL). A moderate correlation between MCQs and
   FINAL (r=0.353 and 0.301, respectively), and strong correlation between
   MIDTERM and FINAL assessments (r=0.688 and 0.610, respectively) were
   found in the first two semesters. The MIDTERM-FINAL correlations were
   enhanced for students who scored under 61\% in the MIDTERM (r=0.887 and
   0.717, respectively). Despite limitations, mostly related to
   particularities of the used tests, the analysis revealed an efficient
   method to identify students at risk of failing the FINAL in a TBL-based
   anatomy program. Future developments include the elaboration of
   strategies to predict and support those underperforming students. Anat
   Sci Educ 8: 413-420. (c) 2014 American Association of Anatomists.},
