@article{ WOS:000821527500001,
Author = {Chen, Qimei and Xu, Xiaoxia and You, Zehua and Jiang, Hao and Zhang, Jun
   and Wang, Fei-Yue},
Title = {Communication-Efficient Federated Edge Learning for NR-U-Based IIoT
   Networks},
Journal = {IEEE INTERNET OF THINGS JOURNAL},
