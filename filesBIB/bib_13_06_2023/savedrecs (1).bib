Title = {Client Scheduling and Resource Management for Efficient Training in
   Heterogeneous IoT-Edge Federated Learning},
Journal = {IEEE TRANSACTIONS ON COMPUTER-AIDED DESIGN OF INTEGRATED CIRCUITS AND
