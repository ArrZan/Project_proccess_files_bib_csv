@article{ WOS:000389843700017,
Author = {Islam, Mohammed A. and Khan, Seher A. and Talukder, Rahmat M.},
Title = {Status of physiology education in US Doctor of Pharmacy programs},
Journal = {ADVANCES IN PHYSIOLOGY EDUCATION},
