@article{ WOS:000950531000001,
Author = {Li, Menghua and Wang, Ding and Zhao, Mingming and Qiao, Junfei},
Title = {Event-triggered constrained neural critic control of nonlinear
   continuous-time multiplayer nonzero-sum games},
Journal = {INFORMATION SCIENCES},
