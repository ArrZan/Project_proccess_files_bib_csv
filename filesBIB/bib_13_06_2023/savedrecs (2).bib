@article{ WOS:000464889800004,
Author = {Eaton, Megan and Scully, Rebecca and Schuller, Mary and Yang, Amy and
   Smink, Douglas and Williams, Reed G. and Bohnen, Jordan D. and George,
   Brian C. and Fryer, Jonathan P. and Meyerson, Shari L.},
Title = {Value and Barriers to Use of the SIMPL Tool for Resident Feedback},
