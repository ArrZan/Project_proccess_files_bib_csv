@article{ WOS:000902002300001,
Author = {Esmaeel Nezhad, Ali and Nardelli, Pedro H. J. and Sahoo, Subham and
   Ghanavati, Farideh},
Title = {Scheduling of Energy Hub Resources Using Robust Chance-Constrained
   Optimization},
