@article{ WOS:000806374200008,
Author = {Ahmad, Maaz and Karimi, Iftekhar A.},
Title = {Families of similar surrogate forms based on predictive accuracy and
   model complexity},
Journal = {COMPUTERS \& CHEMICAL ENGINEERING},
Year = {2022},
Volume = {163},
Month = {JUL},
Abstract = {The prevalence of many diverse surrogate forms and absence of common
   insights to identify the best form make exhaustive, enumerative, and
   tedious search imperative for surrogate selection in most applications.
   In this work, we aim to develop some insights into the similarities
   between varied surrogate forms, which could ease and limit the surrogate
   selection process. We present a modified index to judge such similarity,
   and use 49 forms, two quality metrics, many noisy and non-noisy data
   sets to identify families of similar forms by solving a series of
   mixed-integer linear programs. All our generated families were
   successfully validated using several additional data sets. We propose a
   faster family-based strategy for surrogate selection. The best form
   identified by this strategy is either the same as or performs very much
   like the true best form. This work sheds useful light on the performance
   of various surrogates, and expedites surrogate selection.(c) 2022
