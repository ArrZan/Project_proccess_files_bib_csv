@article{ WOS:000574139500002,
Author = {Ezelle, Heather J. and Geiman, Theresa and Schnaper, Lauren A. and
   Cullen, Kevin J. and Lapidus, Rena S. and Hassel, Bret A.},
Title = {A Translational Approach to Cancer Research, Education and Training},
