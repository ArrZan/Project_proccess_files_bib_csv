@article{ WOS:000524749000037,
Author = {Huang, Yuan and Cheng, Zhe and Zhou, Qianyu and Xiang, Yuxing and Zhao,
   Ruixiao},
Title = {Data Mining Algorithm for Cloud Network Information Based on Artificial
   Intelligence Decision Mechanism},
