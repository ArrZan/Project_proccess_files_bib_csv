@article{ WOS:000966992700001,
Author = {Beurer-Kellner, Luca and von Pilgrim, Jens and Tsigkanos, Christos and
   Kehrer, Timo},
Title = {A Transformational Approach to Managing Data Model Evolution of Web
   Services},
Journal = {IEEE TRANSACTIONS ON SERVICES COMPUTING},
