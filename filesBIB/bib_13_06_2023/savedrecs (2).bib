@article{ WOS:000722157000001,
Author = {Griffiths, Ryan-Rhys and Aldrick, Alexander A. and Garcia-Ortegon,
   Miguel and Lalchand, Vidhi and Lee, Alpha A.},
Title = {Achieving robustness to aleatoric uncertainty with heteroscedastic
   Bayesian optimisation},
