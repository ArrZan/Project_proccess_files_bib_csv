@article{ WOS:000455017600006,
Author = {Kern, Ben D. and Richards, K. Andrew R. and Ayers, Suzan F. and Killian,
   Chad M.},
Title = {Chapter 6: Recruitment in PETE: Interview Results and Discussion},
Journal = {JOURNAL OF TEACHING IN PHYSICAL EDUCATION},
