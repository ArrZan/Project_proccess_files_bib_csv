@article{ WOS:000341835200003,
Author = {Utkin, Lev V. and Zhuk, Yulia A.},
Title = {Imprecise prior knowledge incorporating into one-class classification},
Journal = {KNOWLEDGE AND INFORMATION SYSTEMS},
Year = {2014},
Volume = {41},
Number = {1},
Pages = {53-76},
Month = {OCT},
Abstract = {An extension of Campbell and Bennett's novelty detection or one-class
   classification model incorporating prior knowledge is studied in the
   paper. The proposed extension relaxes the strong assumption of the
   empirical probability distribution over elements of a training set and
   deals with a set of probability distributions produced by prior
   knowledge about training data. The classification problem is solved by
   considering extreme points of the probability distribution set or by
   means of the conjugate duality technique. Special cases of prior
   knowledge are considered in detail, including the imprecise
   linear-vacuous mixture model and interval-valued moments of feature
   values. Numerical experiments show that the proposed models outperform
   Campbell and Bennett's model for many real and synthetic data.},
