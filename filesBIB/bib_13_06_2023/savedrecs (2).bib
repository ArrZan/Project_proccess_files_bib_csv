@article{ WOS:000530011400009,
Author = {Corple, Danielle J. and Zoltowski, Carla B. and Kenny Feister, Megan and
   Buzzanell, Patrice M.},
Title = {Understanding ethical decision-making in design},
Journal = {JOURNAL OF ENGINEERING EDUCATION},
