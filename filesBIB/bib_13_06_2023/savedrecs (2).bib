@article{ WOS:000623215600003,
Author = {Nomura, Osamu and Onishi, Hirotaka and Park, Yoon Soo and Michihata,
   Nobuaki and Kobayashi, Tohru and Kaneko, Kazunari and Yoshikawa,
   Tetsushi and Ishiguro, Akira and Japan Pediat Soc Steering Comm},
Title = {Predictors of performance on the pediatric board certification
   examination},
Journal = {BMC MEDICAL EDUCATION},
Year = {2021},
Volume = {21},
Number = {1},
Month = {FEB 22},
Abstract = {BackgroundExamining the predictors of summative assessment performance
   is important for improving educational programs and structuring
   appropriate learning environments for trainees. However, predictors of
   certification examination performance in pediatric postgraduate
   education have not been comprehensively investigated in Japan.MethodsThe
   Pediatric Board Examination database in Japan, which includes 1578
   postgraduate trainees from 2015 to 2016, was analyzed. The examinations
   included multiple-choice questions (MCQs), case summary reports, and an
   interview, and the predictors for each of these components were
   investigated by multiple regression analysis.ResultsThe number of
   examination attempts and the training duration were significant negative
   predictors of the scores for the MCQ, case summary, and interview.
   Employment at a community hospital or private university hospital were
   negative predictors of the MCQ and case summary score, respectively.
   Female sex and the number of academic presentations positively predicted
   the case summary and interview scores. The number of research
   publications was a positive predictor of the MCQ score, and employment
   at a community hospital was a positive predictor of the case summary
   score.ConclusionThis study found that delayed and repeated examination
   taking were negative predictors, while the scholarly activity of
   trainees was a positive predictor, of pediatric board certification
   examination performance.},
