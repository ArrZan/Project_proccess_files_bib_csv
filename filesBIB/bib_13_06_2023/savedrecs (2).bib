@article{ WOS:000502007000035,
Author = {Fang, Jiayan and Liu, Qiao and Qin, Zhiguang},
Title = {Iterative tighter nonparallel hyperplane support vector clustering with
   simultaneous feature selection},
Journal = {CLUSTER COMPUTING-THE JOURNAL OF NETWORKS SOFTWARE TOOLS AND
   APPLICATIONS},
