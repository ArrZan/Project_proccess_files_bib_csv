@article{ WOS:000949150900015,
Author = {Seker, Oylum and Cevik, Mucahit and Bodur, Merve and Lee, Young and
   Ruschin, Mark},
Title = {A Multiobjective Approach for Sector Duration Optimization in
   Stereotactic Radiosurgery Treatment Planning},
