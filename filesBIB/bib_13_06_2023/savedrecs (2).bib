@article{ WOS:000402148100009,
Author = {Moazeni, Somayeh and Powell, Warren B. and Defourny, Boris and
   Bouzaiene-Ayari, Belgacem},
Title = {Parallel Nonstationary Direct Policy Search for Risk-Averse Stochastic
   Optimization},
Journal = {INFORMS JOURNAL ON COMPUTING},
