@article{ WOS:000763469700064,
Author = {Johanson, Kelly E. and DeFreece, Cecily B. and Morgan, Kathleen M.},
Title = {Enhancing the Mentoring Experience for Underrepresented Students},
Journal = {JOURNAL OF CHEMICAL EDUCATION},
Year = {2022},
Volume = {99},
